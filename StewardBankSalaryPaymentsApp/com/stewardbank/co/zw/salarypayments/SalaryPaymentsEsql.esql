			ELSE
				CALL INSERT_SB_ISAVE_PAYNET_TRX_TRACKING(Env.UID,'','',inDomainDataRef.RetrievalReferenceNumber_037,evnDataDFDL,inDomainDataRef.ResponseCode_039,Env.FileName,Env.Fields,Env.Amount,Env.Dbt_Acc,Env.Crt_Acc,Env.CsvDate,
				'','','SUCCESS','','SUCCESS',Env.CreationDate,CAST(CURRENT_TIMESTAMP AS TIMESTAMP),'FALSE');
			END IF;
			--CALL INSERT_SB_ISAVE_PAYNET_TRX_TRACKING(UUID,inDomainDataRef.ISO8583_1987.AccountIdentification2_103,CAST(currenttimestamp AS TIMESTAMP FORMAT 'yyyyMMdd HHmmssss'),
			--'','00',getPayLoad(domainName,domainDataRef,encodeRef,ccidRef),'SUCCESS','',CAST(currenttimestamp AS TIMESTAMP FORMAT 'yyyyMMdd HHmmssss'),CAST(InputRoot.Properties.CreationTime AS CHARACTER),'123',Environment.Variables.FileName);
			-- SET OutputRoot.DFDL.ISO8583_1987 = InputRoot.DFDL.ISO8583_1987;
			-- SET OutputRoot.DFDL.ISO8583_1987 = getPayLoad(FIELDNAME(inDomainDataRef.ISO8583_1987),inDomainDataRef.ISO8583_1987,encodeRef,ccidRef);
			CREATE LASTCHILD OF OutputRoot DOMAIN('DFDL') PARSE(ASBITSTREAM(InputRoot.DFDL CCSID ccidRef SET '{ISO8583_Lib}' TYPE '{}:ISO8583_1987') ENCODING encodeRef CCSID ccidRef SET '{ISO8583_Lib}' TYPE '{}:ISO8583_1987');

			PROPAGATE TO TERMINAL 'out';
		ELSE
			-- CALL INSERT_SB_ISAVE_PAYNET_TRX_TRACKING(Environment.Variables.UID,'','','',Environment.Variables.zssdfdlData,'',Environment.Variables.FileName,
			-- 'TRANSACTION_TYPE, DEBIT_ACCOUNT, CURRENNCY, AMOUNT, CREDIT_ACCOUNT, COMPANY, DEBIT_DATE, CREDIT_DATE, CREDIT_THEIR_REFERENCE, NARRATION',CAST(inDomainDataRef.AmountTransaction_004 AS CHARACTER),
			-- inDomainDataRef.AccountIdentification2_103,inDomainDataRef.AccountIdentification1_102,Environment.Variables.CsvDate,'','','FAILURE','','SUCCESS',Environment.Variables.CreationDate,CAST(CURRENT_TIMESTAMP AS TIMESTAMP),'');

			IF Env.FileNameCheck LIKE 'PO' THEN
