			-- inDomainDataRef.AccountIdentification2_103,inDomainDataRef.AccountIdentification1_102,Environment.Variables.CsvDate,'','','SUCCESS','','SUCCESS',Environment.Variables.CreationDate,CAST(CURRENT_TIMESTAMP AS TIMESTAMP),'');

